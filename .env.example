# SiliconFlow API (用于 Qwen 生成菜谱)
Qwen_API_KEY=your_siliconflow_api_key
Qwen_BASE_URL=https://api.siliconflow.cn/v1
QWEN_MODEL=<qwen-model-id>
# 兼容写法（如需）：Qwen_MODEL=<qwen-model-id>
# 如你使用 Qwen_* 命名：
# Qwen_API_KEY=your_siliconflow_api_key
# Qwen_BASE_URL=https://api.siliconflow.cn/v1
# Qwen_MODEL=<qwen-model-id>

# DeepSeek API (用于文案撰写)
DEEPSEEK_API_KEY=your_deepseek_api_key
DEEPSEEK_BASE_URL=https://api.deepseek.com/v1
DEEPSEEK_MODEL=<deepseek-model-id>

# LongCat API (用于内容审核)
LONGCAT_API_KEY=your_longcat_api_key
LONGCAT_BASE_URL=https://api.longcat.chat/openai
LONGCAT_MODEL=<longcat-model-id>

# 火山引擎 API (用于文生图)
DOUBAO_API_KEY=your_doubao_api_key
DOUBAO_BASE_URL=https://ark.cn-beijing.volces.com/api/v3
DOUBAO_IMAGE_MODEL=<doubao-image-model-id>
DOUBAO_IMAGE_SIZE=1080x1920

# THUDM/GLM API (用于发布到小红书)
GLM_API_KEY=your_glm_api_key
GLM_BASE_URL=https://api.siliconflow.cn/v1
GLM_MODEL=<glm-model-id>

# 可选：小红书 MCP
XIAOHONGSHU_MCP_URL=http://localhost:18060/mcp
